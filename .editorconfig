# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_trailing_newline = true
indent_size = 4
indent_style = space
charset = utf-8
trim_trailing_whitespace = true

# Match exactly .travis.yml
[{.travis.yml}]
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false
